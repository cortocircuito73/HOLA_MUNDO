// HOLA MUNDO
Algoritmo sin_titulo
	Definir titulo Como Caracter
	Definir pos Como Entero
	titulo <- 'HOLA MUNDO'
	pos <- Longitud(titulo)
	pos <- pos+10
	Para i<-0 Hasta pos Hacer
		Escribir ' ' Sin Saltar
		Si i==5 Entonces
			Escribir titulo Sin Saltar
		FinSi
		Escribir ' ' Sin Saltar
	FinPara
	Escribir ''
FinAlgoritmo
